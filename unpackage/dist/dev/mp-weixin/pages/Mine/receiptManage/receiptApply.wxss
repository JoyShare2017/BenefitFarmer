
page {
	background-color: #ebeef1;
}
.receiptKnow {
	height: 46rpx;
	color: rgb(255, 86, 81);
	line-height: 46rpx;
	width: 140rpx;
	font-size: 28rpx;
	padding-left: 10rpx;
	padding-right: 10rpx;
	margin-top: 17rpx;
	background-color: transparent;
}
.receiptKnow::after {
	border-color: rgb(255, 86, 81);
}
.cellHolder {
	margin-top: 10rpx;
	width: calc(100% - 40rpx);
	background-color: #ffffff;
	border-radius: 10rpx;
	margin-bottom: 10rpx;
}
.address {
	height: 50rpx;
	line-height: 50rpx;
	font-size: 32rpx;
	padding-left: 30rpx;
}
.bottom_tool {
	width: 100%;
	height: 100rpx;
	background-color: #f6f6f6;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	position: fixed;
	border-top: 1rpx solid #616262;
	bottom: calc(0px);
}
.editBtnImg {
	margin-top: 30rpx;
	width: 40rpx;
	height: 40rpx;
	line-height: 99rpx;
}
.bigBtn {
	width: 40%;
	height: 100rpx;
	line-height: 100rpx;
	font-size: 32rpx;
	text-align: center;
	border-left: 1rpx solid #616262;
}

